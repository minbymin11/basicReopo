package variabledeclare;

public class WhileNested {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/**
		  ¡Ú¡Ú¡Ú¡Ú¡Ú\n
		  
		 */
		
		System.out.print("sample ¡Ú¡Ú¡Ú¡Ú¡Ú");
		System.out.print("\n");

		int i = 0;
		
		while (i < 5) {
			int j = 0;
			while (j < 5) {
				System.out.print("¡Ú");
				j++;
			}
			System.out.println("");
			i++;
		}

	/*	
		¡Ù¡Ú¡Ú¡Ú¡Ú
		¡Ú¡Ù¡Ú¡Ú¡Ú
		¡Ú¡Ú¡Ù¡Ú¡Ú
		¡Ú¡Ú¡Ú¡Ù¡Ú
		¡Ú¡Ú¡Ú¡Ú¡Ù
*/
		
		System.out.println("");
		i = 0;

		while (i < 5) {
			int j = 0;
			while (j < 5) {
				if (i != j) {
					System.out.print("¡Ú");
				} else {
					System.out.print("¡Ù");
				}
				j++;
			}
			System.out.println("");
			i++;
		}

	}

}

