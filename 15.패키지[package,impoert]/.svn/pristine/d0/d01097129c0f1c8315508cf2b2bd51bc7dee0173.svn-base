import com.itwill.shop.ShopManager;
import com.itwill.shop.board.Board;
import com.itwill.shop.board.BoardManager;
import com.itwill.shop.member.Member;
import com.itwill.shop.member.MemberManager;


public class ShoppingMallImportMain {

	public static void main(String[] args) {
		/*
		 * import
		 * ctrl+shift+o
		 * ctrl+shift+m
		 */
		ShopManager sm = new ShopManager();
		
		Member member = new Member();
		MemberManager memberManager = new MemberManager();
		
		Board board = new Board();
		BoardManager boardManager = new BoardManager();
		
		//이름이 같아서 줄일수없다(import가 안됨)
		com.itwill.shop.admin.Board adminBoard = new com.itwill.shop.admin.Board();
		com.itwill.shop.admin.BoardManager adminBoardManager = new com.itwill.shop.admin.BoardManager();
		
		System.out.println(sm);
		System.out.println(member);
		System.out.println(memberManager);
		System.out.println(board);
		System.out.println(boardManager);
		System.out.println(adminBoard);
		System.out.println(adminBoardManager);
		
	}

}
